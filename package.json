  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ICEInformaticsDozio/dicomi-etl-erogazioni/issues"
  },
  "homepage": "https://github.com/ICEInformaticsDozio/dicomi-etl-erogazioni#readme",
  "dependencies": {
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "fast-xml-parser": "^5.2.3",
    "mssql": "^11.0.1",
    "nodemailer": "^7.0.3",
    "nodemon": "^3.1.10",
    "pm2": "^6.0.6",
    "tedious": "^18.6.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/mssql": "^9.1.7",
